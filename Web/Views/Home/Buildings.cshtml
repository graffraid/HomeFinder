@using Domain.Entities
@model List<Building>

<div class="row">
    <div class="col-md-4">
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>No</th>
                    <th>Street</th>
                    <th>Number</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < Model.Count; i++)
                {
                    <tr>
                        <td>@i</td>
                        <td>@Model[i].Street</td>
                        <td>@Model[i].No</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="col-md-8" id="map" style="width:600px; height:450px">
    </div>
</div>
